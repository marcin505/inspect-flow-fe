{
  "name": "inspection-tool",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite optimize && vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "test": "vitest --reporter verbose",
    "host": "vite --host",
    "coverage": "vitest run --coverage",
    "tsc-check": "tsc --noEmit",
    "lint": "eslint src/",
    "lint-fix": "eslint src/ --fix",
    "prettier-check": "prettier --check src/**/*.{ts,tsx,js,jsx,css}",
    "prettier-format": "prettier --write src/**/*.{ts,tsx,js,jsx,css}",
    "prepare": "husky install",
    "pre-commit": "npm run tsc-check && npm run lint && npm run prettier-check",
    "pre-push": "npm run coverage",
    "bump-version": "npm version patch --message \"chore: bump version to %s\""
  },
  "dependencies": {
    "@azure/msal-browser": "^3.23.0",
    "@azure/msal-react": "^2.0.22",
    "@headlessui/react": "^2.2.0",
    "@microsoft/applicationinsights-react-js": "^3.4.2",
    "@microsoft/applicationinsights-web": "^3.0.0",
    "@tanstack/react-table": "^8.7.9",
    "axios": "^1.7.4",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "focus-trap-react": "^10.1.1",
    "history": "^5.3.0",
    "jotai": "^2.0.3",
    "jwt-decode": "^3.1.2",
    "msw": "^1.1.0",
    "react": "^18.2.0",
    "react-calendar": "^5.0.0",
    "react-datepicker": "^7.4.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.2.10",
    "react-papaparse": "^4.4.0",
    "react-query": "^3.39.3",
    "react-resize-detector": "^10.0.1",
    "react-router": "^6.22.0",
    "react-router-dom": "^6.22.0",
    "react-tabs": "^6.0.2",
    "react-toastify": "^10.0.5",
    "react-tooltip": "^5.26.2",
    "replace-in-file": "^7.0.1",
    "styled-components": "^6.1.8",
    "use-callback-ref": "^1.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@faker-js/faker": "^7.6.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/lodash": "^4.17.7",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.5",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest-styled-components": "^7.2.0",
    "jsdom": "^21.1.0",
    "lodash": "^4.17.21",
    "papaparse": "^5.4.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^5.4.11",
    "vitest": "^2.1.5"
  },
  "overrides": {
    "esbuild": "^0.24.0",
    "cross-spawn": "7.0.6"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
